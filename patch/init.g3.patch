chmod +x 0755 /sbin/nebulaketnel-perf_boot.sh
chmod +x 0755 /sbin/nebulaketnel-post_boot.sh

service nebulakernel-perf-boot /system/bin/sh /sbin/nebulakernel-perf_boot.sh
    class core
    user root
    disabled
    oneshot

service nebulakernel-post-boot /system/bin/sh /sbin/nebulakernel-post_boot.sh
    class late_start
    user root
    disabled
    oneshot

start nebulakernel-perf-boot

on property:sys.boot_completed=1
    start nebulakernel-post-boot
